pipeline {
  agent any
  tools {
    maven 'M3' 
  }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Checkout') {
      steps {
        git 'https://github.com/itsmouhy/AssafaBanking.git'
      }
    }
    stage('Build') {
      steps {
        sh 'chmod +x ./mvnw' 
        sh './mvnw clean install'
      }
    }
  }
}
